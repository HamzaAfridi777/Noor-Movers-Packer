    <script>
      function submitForm() {
        document.getElementById("contact-form1");
        const e = document.getElementById("successMessage");
        (e.style.display = "block"),
          setTimeout(function () {
            e.style.opacity = "1";
          }, 100),
          setTimeout(function () {
            e.style.opacity = "0";
          }, 5e3);
      }
      function toggleFields() {
        "Suggestion" === $("#play_type").val()
          ? $("#Suggestions").show()
          : $("#Suggestions").hide(),
          "Inquiry" === $("#play_type").val()
            ? $("#Inquirys").show()
            : $("#Inquirys").hide(),
          "GeneralQuires" === $("#play_type").val()
            ? $("#General").show()
            : $("#General").hide(),
          "History" === $("#play_type").val()
            ? $("#History-1").show()
            : $("#History-1").hide(),
          "Custom" === $("#play_type").val()
            ? $("#Custom-1").show()
            : $("#Custom-1").hide();
      }
      $(document).ready(function () {
        $(document).on("submit", "#contact-form1", function (e) {
          return "" == $("#name").val() ||
            "" == $("#email").val() ||
            "" == $("#query_type").val() ||
            "" == $("#phone").val() ||
            "" == $("#message").val()
            ? (e.preventDefault(), alert("Fill all fields first"), !1)
            : "" === $("#g-recaptcha-response").val()
            ? (e.preventDefault(), alert("Please check the recaptcha"), !1)
            : void $("#contact-form1").submit();
        });
      }),
        $(document).ready(function () {
          toggleFields(),
            $("#play_type").change(function () {
              toggleFields();
            });
        });
    </script>
